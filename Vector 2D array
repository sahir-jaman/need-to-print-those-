//Vector 2D array

#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<vector<int> >v;
    for(int i=0;i<5;i++)
    {
        vector<int>temp;
        for(int j=0;j<5;j++)
        {

            temp.push_back(i);
        }
        v.push_back(temp);
    }

    for(int i=0;i<v.size();i++)
    {
        for(int j=0;j<v[i].size();j++)
        {
            cout<<v[i][j]<<" ";
        }
        cout<<endl;
    }



    return 0;
}
